{
  "name": "am-taro-form",
  "version": "0.0.1",
  "description": "基于taro的跨平台Form表单库",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "Sass",
    "framework": "React"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "taro",
    "form",
    "nutui-react-taro",
    "am-taro-form",
    "跨平台表单"
  ],
  "scripts": {
    "build": "father build",
    "build:h5": "taro build --type h5",
    "dev:h5": "npm run build:h5 -- --watch",
    "prepare": "husky"
  },
  "browserslist": {
    "development": [
      "defaults and fully supports es6-module",
      "maintained node versions"
    ],
    "production": [
      "last 3 versions",
      "Android >= 4.1",
      "ios >= 8"
    ]
  },
  "author": "Armon",
  "dependencies": {
    "@nutui/nutui-react-taro": "^2.0.0",
    "@tarojs/components": "4.0.9",
    "@tarojs/plugin-platform-h5": "4.0.9",
    "@tarojs/react": "4.0.9",
    "@tarojs/runtime": "4.0.9",
    "@tarojs/shared": "4.0.9",
    "@tarojs/taro": "4.0.9",
    "rc-field-form": "^2.7.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/preset-react": "^7.24.1",
    "@babel/runtime": "^7.24.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@tarojs/cli": "4.0.9",
    "@tarojs/helper": "4.0.9",
    "@tarojs/plugin-framework-react": "4.0.9",
    "@tarojs/plugin-html": "^4.0.10",
    "@tarojs/plugin-platform-alipay": "4.0.9",
    "@tarojs/plugin-platform-harmony-hybrid": "4.0.9",
    "@tarojs/plugin-platform-jd": "4.0.9",
    "@tarojs/plugin-platform-qq": "4.0.9",
    "@tarojs/plugin-platform-swan": "4.0.9",
    "@tarojs/plugin-platform-tt": "4.0.9",
    "@tarojs/plugin-platform-weapp": "4.0.9",
    "@tarojs/taro-loader": "4.0.9",
    "@tarojs/webpack5-runner": "4.0.9",
    "@types/node": "^18",
    "@types/react": "^18.0.0",
    "@types/webpack-env": "^1.13.6",
    "babel-plugin-import": "^1.13.8",
    "babel-preset-taro": "4.0.9",
    "eslint": "^8.57.0",
    "eslint-config-taro": "4.0.9",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.4.0",
    "father": "^4.5.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "postcss": "^8.4.38",
    "prettier": "^3.5.3",
    "react-refresh": "^0.14.0",
    "stylelint": "^16.4.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.4.5",
    "webpack": "5.91.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,tsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  }
}
